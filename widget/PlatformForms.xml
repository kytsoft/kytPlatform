<?xml version="1.0" encoding="UTF-8"?>
<forms xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:noNamespaceSchemaLocation="http://ofbiz.apache.org/dtds/widget-form.xsd">

	<form name="ListFormGroup" type="list" list-name="formGroups"
		target="editFormGroup" odd-row-style="alternate-row" header-row-style="header-row-2"
		default-table-style="basic-table hover-bar">
		<actions>
			<entity-condition entity-name="FormGroup" list="formGroups" />
		</actions>
		<field name="formGroupId">
			<hyperlink target="formManagement" description="${formGroupId}">
				<parameter param-name="fileName"  from-field="fileName"/>
				<parameter param-name="groupId" from-field="formGroupId"/>
			</hyperlink>
		</field>
		<field name="title">
			<display />
		</field>
		<field name="fileName">
			<display />
		</field>
		<field name="submitButton" title="${uiLabelMap.CommonEdit}">
			<submit button-type="text-link" />
		</field>
		<field name="deleteLink" title="${uiLabelMap.CommonDelete}">
			<hyperlink target="removeFormGroup" description="${uiLabelMap.CommonDelete}">
				<parameter param-name="formGroupId" />
			</hyperlink>
		</field>
	</form>

	<form name="EditFormGroup" type="single" target="updateFormGroup"
		title="" default-map-name="formGroup" header-row-style="header-row"
		default-table-style="basic-table">
		<actions>
			<entity-one entity-name="FormGroup" value-field="formGroup" />
		</actions>
		<alt-target use-when="formGroup==null" target="createFormGroup" />
		<field use-when="formGroup!=null" name="formGroupId"
			tooltip="${uiLabelMap.CommonNotModifRecreat}">
			<display also-hidden="true" />
		</field>
		<field name="title" required-field="true">
			<text size="30" maxlength="60" />
		</field>
		<field name="fileName" required-field="true">
			<text size="30" maxlength="60" />
		</field>
		<field name="aa" title="sss">
			<hyperlink target="javascript:set_value('111')" target-type="plain" description="wen"/>
		</field>
		<field name="submitButton" title="${uiLabelMap.CommonSubmit}">
			<submit button-type="button" />
		</field>
	</form>
	<!-- 新建表单form -->	
	<form name="createForms" type="single" target="createFormsubmit" 
		title=""   header-row-style="header-row"
		default-table-style="basic-table">
		<actions>
		<set field="formGroupId" value="${formGroupId}"/>
		<!-- <set field="fileName" value="${fileName}"/> -->
		</actions>
		<alt-target use-when="formGroup==null" target="createFormGroup" />
		<field name="formGroupId" required-field="true"  tooltip="${uiLabelMap.CommonNotModifRecreat}">
			<text size="30" maxlength="60"  default-value="${formGroupId}"  />
			<display also-hidden="true" />
		</field>
 		<field name="fileName" required-field="true" >
			<text size="30" maxlength="60"  default-value="${fileName}" />
			<hidden/>
		</field> 
		<field name="formName" required-field="true" title="${uiLabelMap.FormName}">
			<text size="30" maxlength="60" />
		</field>
		<field name="target" required-field="true" title="${uiLabelMap.target}">
			<text size="30" maxlength="60" />
		</field>
		<field name="submitButton" title="${uiLabelMap.CommonSubmit}">
			<submit button-type="text-link" />
		</field>
	</form>

	<!-- Edit liu jianping -->
<!-- 获取表单信息 -->
<form name="ListForms"  type="list"  list-name="formList"
		target="updateGoogleCheckoutShippingMethod" odd-row-style="alternate-row"
		header-row-style="header-row-2" default-table-style="basic-table hover-bar" >
		<actions>
			<set field="fileNames" value="${fileName}"/>			
			<service service-name="getFormList">
				<field-map field-name="fileName"  from-field="fileNames"/>
			</service>
		</actions>
		<field name="name" title="${uiLabelMap.FormName}">
			<display />
		</field>
	 	 <field name="type"  title="${uiLabelMap.FormType}">
	  		<display />
	  		</field> 
		<field name="alter"  title="${uiLabelMap.FormCtrl}">
		    <hyperlink target="showforminfo"  description="${uiLabelMap.CommonUpdate}" >
                <parameter param-name="formName" from-field="name"/>
                <parameter param-name="fileName"  from-field="fileNames"/>
                <parameter param-name="formGroupId"  from-field="formGroupId"/>
            </hyperlink>
		</field>
		<field name="deleteLink" title="${uiLabelMap.FormCtrl}" widget-style="buttontext">
            <hyperlink target="deleteFormaction" description="${uiLabelMap.CommonDelete}" also-hidden="false">
                 <parameter param-name="formName" from-field="name"/>
                 <parameter param-name="fileName"  from-field="fileNames"/>
                <parameter param-name="formGroupId"  from-field="formGroupId"/>
            </hyperlink>
        </field>
	</form>
	<form name="setformAttribute" type="list" list-name="dates"
		target="aaa" odd-row-style="alternate-row" header-row-style="header-row-2"
		default-table-style="basic-table hover-bar">
		<actions>
			<service service-name="getformAttributes">
				<field-map field-name="formName" value="${formName}" />
			</service>
		</actions>
		<field name="name"> <!-- qqq sdfds 111-->
			<display></display>
		</field>
		<field name="value" title="${uiLabelMap.AccountingAgreementTypeId}">
			<drop-down allow-empty="true">
				<entity-options description="${description}"
					entity-name="AgreementType" key-field-name="agreementTypeId" />
			</drop-down>
		</field>

		<field name="submitButton" title="${uiLabelMap.CommonUpdate}">
			<submit button-type="button" />
		</field>
	</form>
	<form name="updateForms" type="single" target="formsubmit"
		title=""   header-row-style="header-row"
		default-table-style="basic-table">
		<actions>
		<set  field="formId" value="${formAtt.formId}"/>
		</actions>
		<alt-target use-when="formId==null" target="createFormGroup" />
		<field name="formId" required-field="true"  attribute-name="formAtt.formId"  tooltip="${uiLabelMap.CommonNotModifRecreat}">
			<display  also-hidden="true"/>
		</field>
		<field name="fileName" required-field="true"  attribute-name="formAtt.fileName">
			<text size="30" maxlength="60"  default-value="${formAtt.fileName}" />
			<hidden />
		</field>
		<field name="formGroupId" required-field="true"  attribute-name="formAtt.fileName">
			<text size="30" maxlength="60"  default-value="${formAtt.formGroupId}" />
			<hidden />
		</field>
		<field name="formName" required-field="true" >
			<text size="30" maxlength="60"  default-value="${formAtt.formName}" />
		</field>
		<field name="target" required-field="true">
			<text size="30" maxlength="60" default-value="${formAtt.formtarget}" />
		</field>
		<!-- <field name="formType" required-field="true">
			<text size="30" maxlength="60" default-value="${formAtt.formType}"/>
		</field> -->
		<field name="submitButton" title="${uiLabelMap.CommonSubmit}">
			<submit button-type="text-link" />
		</field>
	</form>
		<!-- 修改表单属性form -->
<form name="setformAttribute" type="list" list-name="dates"   
		target="aaa" odd-row-style="alternate-row"
		header-row-style="header-row-2" default-table-style="basic-table hover-bar">
		<actions>
		<service service-name="getformAttributes">
			<field-map field-name="formName"  value="${formName}"/>
		</service>	
		</actions>
		<field name="name"  ><display></display></field>
		<field name="value" 
		       title="${uiLabelMap.AccountingAgreementTypeId}">
		       <drop-down allow-empty="true">
		            <entity-options
		             description="${description}"
		             entity-name="AgreementType"
		             key-field-name="agreementTypeId" />
		       </drop-down>
		</field>
		
		<field name="submitButton" title="${uiLabelMap.CommonUpdate}">
			<submit button-type="button" />
		</field>
	</form>
</forms>