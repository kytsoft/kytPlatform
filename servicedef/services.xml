<?xml version="1.0" encoding="UTF-8"?>
<services xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:noNamespaceSchemaLocation="http://ofbiz.apache.org/dtds/services.xsd">
	<description>Platform Services</description>
	<vendor></vendor>
	<version>1.0</version>

	<service name="noSyntaxError" engine="interface">
		<description>
            Dummy service to prevent empty files and syntax error - Remove when the 1st real service will be added here 
        </description>
	</service>

	<service name="getFormList" engine="java"
		location="com.kyt.platform.form.FormServices" invoke="getFormList"
		auth="true">
		 <attribute name="fileName" type="String" mode="IN"/> 
		<attribute name="formList" mode="OUT" type="java.util.List"></attribute>
	</service>

	<!-- 表单管理 -->
	<service name="createFormGroup" engine="java" default-entity-name="FormGroup"
            location="com.kyt.platform.form.FormServices" invoke="createFormGroup" auth="false">
        <auto-attributes mode="IN" include="pk" optional="true"/>
        <auto-attributes mode="OUT" include="pk" optional="false" />
        <auto-attributes mode="IN" include="nonpk" optional="false" />
    </service>
    <service name="updateFormGroup" engine="java" default-entity-name="FormGroup"
            location="com.kyt.platform.form.FormServices" invoke="updateFormGroup" auth="true">
        <auto-attributes mode="IN" include="pk" optional="false"/>
        <auto-attributes mode="IN" include="nonpk" optional="false" />
    </service>
    <service name="removeFormGroup" engine="java" default-entity-name="FormGroup"
            location="com.kyt.platform.form.FormServices" invoke="removeFormGroup" auth="true">
        <auto-attributes mode="IN" include="pk" optional="false"/>
    </service>
	<!-- 测试表单 -->
	<service name="examinationForm" engine="java" location="com.kyt.platform.form.FormServices" invoke="examinationForm" auth="true">
		<attribute name="xmlname" mode="IN" type="java.lang.String" optional="true"></attribute>
		<attribute name="formname" mode="INOUT" type="java.lang.String"></attribute>
		<attribute name="xmlpath" mode="OUT" type="java.lang.String"></attribute>
		<attribute name="findform" mode="OUT" type="java.lang.String"></attribute>
	</service>

</services>